{
  "name": "portal-lojista",
  "version": "1.0.0",
  "description": "Projeto do Portal lojista",
  "main": "serverless.ts",
  "scripts": {
    "test:unit": "yarn test --watch -c jest-unit-config.js",
    "test": "jest --noStackTrace --runInBand --detectOpenHandles --passWithNoTests --silent",
    "dev": "sls offline start -s dev --reloadHandler",
